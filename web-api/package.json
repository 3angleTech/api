{
  "name": "web-api",
  "version": "1.0.0-dev",
  "description": "Open Source NodeJS Project w/ TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "lint": "tslint --project tsconfig.json",
    "prepare-db": "sequelize db:migrate && sequelize db:seed:all",
    "prepare-db-and-start": "npm run prepare-db && npm run start",
    "start": "node dist/index.js",
    "test": "npm run build && NODE_ENV=test npm run prepare-db && NODE_ENV=test nyc mocha dist/**/*.spec.js dist/common/**/*.spec.js dist/modules/**/*.spec.js --exit"
  },
  "repository": {
    "type": "git",
