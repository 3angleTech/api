{
  "name": "web-api",
  "version": "1.0.0-dev",
  "description": "Open Source NodeJS Project w/ TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "lint": "tslint --project tsconfig.json",
    "prepare-db": "sequelize db:migrate && sequelize db:seed:all",
    "prepare-db-and-start": "npm run prepare-db && npm run start",
    "start": "node dist/index.js",
    "test": "npm run build && NODE_ENV=test npm run prepare-db && NODE_ENV=test nyc mocha dist/**/*.spec.js dist/common/**/*.spec.js dist/modules/**/*.spec.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3angleTech/webApi.git"
  },
  "author": "3angleTech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3angleTech/webApi/issues"
  },
  "homepage": "https://github.com/3angleTech/webApi#readme",
  "dependencies": {
    "@sendgrid/client": "6.3.0",
    "@sendgrid/mail": "6.3.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "config": "3.0.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "http-status-codes": "1.3.0",
    "inversify": "5.0.1",
    "json2typescript": "1.0.6",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.11",
    "oauth2-server": "3.0.1",
    "path": "0.12.7",
    "pg": "7.6.1",
    "reflect-metadata": "0.1.12",
    "sequelize": "5.15.1"
  },
  "devDependencies": {
    "@3angletech/tslint": "1.0.7",
    "@types/bcrypt": "3.0.0",
